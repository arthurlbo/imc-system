#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/arthurlobo/projects/imc-system/server/node_modules/.pnpm/typeorm@0.3.24_reflect-metadata@0.2.2_sqlite3@5.1.7/node_modules/typeorm/node_modules:/home/arthurlobo/projects/imc-system/server/node_modules/.pnpm/typeorm@0.3.24_reflect-metadata@0.2.2_sqlite3@5.1.7/node_modules:/home/arthurlobo/projects/imc-system/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/arthurlobo/projects/imc-system/server/node_modules/.pnpm/typeorm@0.3.24_reflect-metadata@0.2.2_sqlite3@5.1.7/node_modules/typeorm/node_modules:/home/arthurlobo/projects/imc-system/server/node_modules/.pnpm/typeorm@0.3.24_reflect-metadata@0.2.2_sqlite3@5.1.7/node_modules:/home/arthurlobo/projects/imc-system/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli-ts-node-commonjs.js" "$@"
else
  exec node  "$basedir/../../cli-ts-node-commonjs.js" "$@"
fi
